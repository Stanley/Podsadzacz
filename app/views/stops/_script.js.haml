  var map = root.map

  $.jGrowl("Kliknij poniżej, aby zbliżyć widok na przystanek: #{ @stop.name }.<br /><br />Skala: <a href='javascript: map.setZoom(17)'>50m</a> <a href='javascript: map.setZoom(16)'>100m</a> <a href='javascript: map.setZoom(15)'>200m</a>.")
